{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    ["module-resolver", {
     "alias": {
       "@src": "./src",
       "@Components": "./src/components",
       "@Charts": "./src/components/charts",
       "@Cards": "./src/components/card"
     }
   }]
  ]
} 