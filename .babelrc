{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current"
      }
    }],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    ["module-resolver", {
      "alias": {
        "@src": "./src",
        "@Components": "./src/components",
        "@Charts": "./src/components/charts",
        "@Cards": "./src/components/card",
        "@Styles": "./src/assets/styles"
      }
    }]
  ]
}
