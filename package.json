{
  "name": "MDwalks-UI",
  "version": "0.12.4",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "build": "rollup -c",
    "build:docz": "rm -rf docs && yarn docz build",
    "lint": "eslint ./src/**/*.js",
    "lint:fix": "eslint ./src/**/*.js --fix",
    "version": "yarn build && git add ./dist && conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.docz/",
      "<rootDir>/src/components/__tests__/__snapshots__/"
    ],
    "setupFilesAfterEnv": [
      "./setupTest.js"
    ],
    "moduleNameMapper": {
      "^@src(.*)$": "<rootDir>/src/$1",
      "^@Components(.*)$": "<rootDir>/src/components/$1",
      "^@Charts(.*)$": "<rootDir>/src/components/charts/$1",
      "^@Cards(.*)$": "<rootDir>/src/components/card/$1",
      "^@Table(.*)$": "<rootDir>/src/components/table/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass)$": "identity-obj-proxy"
    }
  },
  "husky": {
    "hooks": {
      "**comment_pre-commit**": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "**comment_lint-staged**": {
    "./src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": "https://github.com/linewalks/MDwalks-UI.git",
  "keywords": [
    "data",
    "react",
    "charts"
  ],
  "author": "linewalks",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "docz": "^2.0.0-rc.67",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-adapter-react-16.3": "^1.7.2",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gatsby-plugin-asset-path": "^1.0.2",
    "gatsby-source-filesystem": "^2.1.22",
    "highcharts": "^7.1.2",
    "highcharts-react-official": "^2.2.2",
    "husky": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.7.1",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.4.3",
    "node-sass": "^4.13.1",
    "rollup": "^1.16.7",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svg": "^2.0.0",
    "scheduler": "0.14.0"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "^3.2.0",
    "conventional-changelog-cli": "^2.0.28",
    "d3": "^5.9.7",
    "d3-sankey-circular": "^0.34.0",
    "d3-simple-slider": "^1.5.4",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.8.6",
    "styled-components": "^4.3.2"
  }
}
